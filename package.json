{
  "name": "xdotool",
  "version": "1.0.52",
  "description": "C++ bindings for Node.js to handle X11 using the full power of libxdo.",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "scripts": {
    "postinstall": "npx cmake-js --prefer-clang --out build/cmake-js compile"
  },
  "files": [
    "**/*.{cpp,h,c,map,js,d.ts}",
    "**/CMakeLists.txt",
    "!build",
    "!cmake-build-*"
  ],
  "keywords": [
    "x11",
    "xorg",
    "bindings"
  ],
  "author": {
    "email": "victorcqueirozg@gmail.com",
    "name": "Victor Queiroz",
    "url": "https://victorqueiroz.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.12.12",
    "sarg": "^4.0.2",
    "ts-node": "^8.5.2",
    "typescript": "4.3.x"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "6.2.x",
    "nan": "2.14.x",
    "tslib": "2.2.x"
  }
}
